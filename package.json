  },
  "homepage": "https://github.com/fugwenna/bunkbotjs#readme",
  "dependencies": {
    "colors": "^1.4.0",
    "discord.js": "^13.2.0",
    "pouchdb": "^7.2.2",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@types/pouchdb": "^6.4.0"
  }
